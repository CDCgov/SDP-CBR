    </dependency>
    <dependency>
      <groupId>org.apache.camel</groupId>
      <artifactId>camel-hl7</artifactId>
      <version>2.18.1.redhat-000012</version>
    </dependency>
    <dependency>
      <groupId>org.apache.camel</groupId>
